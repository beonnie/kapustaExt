{
    "default_locale": "en",
    "name": "__MSG_name__",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "__MSG_description__",
    "icons": {
        "32":"./icons/main32.png",
        "64":"./icons/main64.png",
        "128":"./icons/main128.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "run_at": "document_idle",
          "all_frames": false,
          "js": ["js/content_script.js"]
        }
      ],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "/html/popup.html"
    }
}